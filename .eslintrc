{
	"parser": "babel-eslint",
	"env": {
		"es6": true,
		"browser": true,
		"node": true
	},
	"rules": {
		"semi": "off",
		"arrow-parens": "off",
		"comma-dangle": "off",
		"no-tabs": "off",
		"indent": ["error", "tab"],
		"require-jsdoc": "off"
	},
	"extends": [
		"eslint:recommended",
		"google"
	]
}